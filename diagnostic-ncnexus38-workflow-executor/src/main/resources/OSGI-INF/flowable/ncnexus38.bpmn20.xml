<?xml version="1.0" encoding="UTF-8" ?>
<definitions id="definitions" xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:flowable="http://flowable.org/bpmn"
  targetNamespace="http://flowable.org/bpmn20" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.omg.org/spec/BPMN/20100524/MODEL http://www.omg.org/spec/BPMN/2.0/20100501/BPMN20.xsd">

  <process id="ncnexus38_diagnostic_binning" name="NCNEXUS38DiagnosticBinning">

    <startEvent id="start" />
    <serviceTask id="initialize" flowable:class="org.renci.canvas.binning.core.diagnostic.InitializeDelegate" />
    <!-- <serviceTask id="loadVCF" flowable:class="org.renci.canvas.binning.diagnostic.ncnexus38.executor.LoadVCFDelegate" /> -->
    <!-- <serviceTask id="loadCoverage" flowable:class="org.renci.canvas.binning.diagnostic.ncnexus38.executor.LoadCoverageDelegate" /> -->
    <!-- <serviceTask id="updateFrequencies" flowable:class="org.renci.canvas.binning.diagnostic.ncnexus38.executor.UpdateFrequenciesDelegate" 
      /> -->
    <serviceTask id="annotateVariants" flowable:class="org.renci.canvas.binning.diagnostic.ncnexus38.executor.AnnotateVariantsDelegate" />
    <serviceTask id="updateDiagnosticBinsHGMD" flowable:class="org.renci.canvas.binning.diagnostic.ncnexus38.executor.UpdateDiagnosticBinsHGMDDelegate" />
    <serviceTask id="updateDiagnosticBinsClinVar" flowable:class="org.renci.canvas.binning.diagnostic.ncnexus38.executor.UpdateDiagnosticBinsClinVarDelegate" />
    <serviceTask id="generateReport" flowable:class="org.renci.canvas.binning.diagnostic.ncnexus38.executor.GenerateReportDelegate" />
    <serviceTask id="finalize" flowable:class="org.renci.canvas.binning.core.diagnostic.FinalizeDelegate" />
    <endEvent id="end" />

    <sequenceFlow sourceRef="start" targetRef="initialize" />
